{
  "name": "bdd-parser",
  "version": "0.0.0-development",
  "description": "Write BDD tests in Markdown.",
  "type": "module",
  "dependencies": {
    "chalk": "5.0.1",
    "tap-parser": "11.0.1"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.122",
    "@types/node": "18.7.16",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "tsx": "3.9.0",
    "typescript": "4.8.2"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "npx tsx tests.ts",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext .js,.ts"
    ]
  }
}
